[Model]
  model = 1phase
[]

[Physics]
  gravity = -9.8 0 0
[]

[Mesh]
  type = UniformMesh
  start = 0 0 0
  length = 1
  n_cell = 50
[]

[EoS]
  [air]
    type = IdealGasEoS
    gamma = 1.0005
    R = 290
  []
[]

[IC]
  p = 1.0e5
  T = 300
  u = 0
[]

[BC]
  [left]
    type = SolidWallBC
    boundary = left
    phase = air
  []
  [right]
    type = SolidWallBC
    boundary = right
    phase = air
  []
[]

[Executioner]
  type = ImplicitEulerExecutioner
  dt = 0.01
