# Models a natural circulation loop.

[Model]
  model = 1phase
[]

[Physics]
  gravity = -9.8 0 0
[]

[HeatTransfer]
  [mesh1]
    T_wall = 350
    htc_wall = 1e5
    P_heat = 1
  []
  [mesh5]
    T_wall = 250
    htc_wall = 1e5
    P_heat = 1
  []
[]

[Mesh]
  [mesh1]
    type = UniformMesh
    start = 0 0 0
    orientation = 1 0 0
    length = 1
    n_cell = 20
  []
  [mesh2]
    type = UniformMesh
    start = 1 0 0
    orientation = 1 0 0
    length = 1
    n_cell = 20
  []
  [mesh3]
    type = UniformMesh
    start = 2 0 0
    orientation = 0 1 0
    length = 1
    n_cell = 20
  []
  [mesh4]
    type = UniformMesh
    start = 2 1 0
    orientation = 0 1 0
    length = 1
    n_cell = 20
  []
  [mesh5]
    type = UniformMesh
    start = 2 2 0
    orientation = -1 0 0
    length = 1
    n_cell = 20
  []
  [mesh6]
    type = UniformMesh
    start = 1 2 0
    orientation = -1 0 0
    length = 1
    n_cell = 20
  []
  [mesh7]
    type = UniformMesh
    start = 0 2 0
    orientation = 0 -1 0
    length = 2
    n_cell = 20
  []
  [mesh8]
    type = UniformMesh
    start = 2 1 0
    orientation = 1 0 0
    length = 1
    n_cell = 20
  []
[]

[EoS]
  [water]
    type = StiffenedGasEoS
    gamma = 2.35
    cv = 1816.0
    q = -1.167e6
    p_inf = 1.0e9
    q_prime = 0
  []
[]

[IC]
  A = 1
  T = 300
  p = 1e5
  u = 0
[]

[BC]
  [pressurizer]
    type = InletP0T0BC
    phase = water
    mesh_name = mesh8
    boundary = right
    p0 = 1e5
    T0 = 300
  []
[]

[Junctions]
  [junction1]
    type = NewerCompressibleJunction
    phase = water
    mesh_names = mesh1 mesh2
    mesh_sides = right left
  []
  [junction2]
    type = NewerCompressibleJunction
    phase = water
    mesh_names = mesh2 mesh3
    mesh_sides = right left
  []
  [junction3]
    type = NewerCompressibleJunction
    phase = water
    mesh_names = mesh3 mesh4 mesh8
    mesh_sides = right left  left
  []
  [junction4]
    type = NewerCompressibleJunction
    phase = water
    mesh_names = mesh4 mesh5
    mesh_sides = right left
  []
  [junction5]
    type = NewerCompressibleJunction
    phase = water
    mesh_names = mesh5 mesh6
    mesh_sides = right left
  []
  [junction6]
    type = NewerCompressibleJunction
    phase = water
    mesh_names = mesh6 mesh7
    mesh_sides = right left
  []
  [junction7]
    type = NewerCompressibleJunction
    phase = water
    mesh_names = mesh7 mesh1
    mesh_sides = right left
  []
[]

[SpatialDiscretization]
  type = FiniteElementMethod
[]

[Executioner]
  type = TransientExecutioner
  ss_tol = 1e-6

  [TimeIntegrator]
    type = ImplicitEuler
  []
  [TimeStepSizer]
    type = ConstantTimeStepSizer
    dt = 1.0
    end_time = 5000.0
  []
[]

[NonlinearSolver]
  verbose = True
  absolute_tolerance = 1e-5
  max_iterations = 10

  scaling_arhoA1  = 1
  scaling_arhouA1 = 1e-1
  scaling_arhoEA1 = 1e-5
[]

[Output]
  [csv]
    type = CSVOutput
    file_name = natural_circulation_loop.csv
    names = rho u p p0 T
    save_by_mesh = True
  []
  [plot_rho_u_T]
    type = PlotOutput
    file_name = natural_circulation_loop_rho_u_T.pdf
    plot_sets = rho u T
    plot_by_mesh = True
  []
  [plot_p_p0]
    type = PlotOutput
    file_name = natural_circulation_loop_p_p0.pdf
    plot_sets = p p0
    plot_by_mesh = True
  []
[]
