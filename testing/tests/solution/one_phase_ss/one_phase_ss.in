[Model]
  model = 1phase
[]

[Physics]
  gravity = 0 0 0
[]

[Mesh]
  type = UniformMesh
  start = 0 0 0
  length = 1
  n_cell = 5
[]

[EoS]
  [air]
    type = IdealGasEoS
    gamma = 1.4
    R = 290
  []
[]

[IC]
  p = 1.0e5
  T = 300
  u = 1
[]

[BC]
  [inlet]
    type = InletRhoUBC
    rho = 5
    u = 1
    boundary = left
    phase = air
  []
  [outlet]
    type = OutletBC
    p = 1.0e5
    boundary = right
    phase = air
  []
[]

[SpatialDiscretization]
  type = FiniteElementMethod
[]

[Executioner]
  type = SteadyStateExecutioner
[]

[NonlinearSolver]
  absolute_tolerance = 1e-5
  max_iterations = 10
[]

[Output]
  [csv]
    type = CSVOutput
    names = rho u p
  []
  [plot]
    type = PlotOutput
    plot_sets = rho u p T
  []
[]
